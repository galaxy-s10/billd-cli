{
  "name": "billd-cli",
  "version": "0.0.11",
  "description": "A cli for quickly create project",
  "keywords": [
    "billd",
    "cli",
    "vue3",
    "react17",
    "vite2",
    "webpack5",
    "template"
  ],
  "homepage": "https://github.com/galaxy-s10/billd-cli",
  "bugs": {
    "url": "https://github.com/galaxy-s10/billd-cli/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/galaxy-s10/billd-cli"
  },
  "license": "ISC",
  "author": "galaxy-s10",
  "bin": {
    "billd": "bin/billd.js"
  },
  "scripts": {
    "changelog": "conventional-changelog -n ./changelog.option.js -p eslint -i CHANGELOG.md -s",
    "changelog:first": "conventional-changelog -n ./changelog.option.js -p eslint -i CHANGELOG.md -s -r 0",
    "commit": "cz",
    "mypublish": "node scripts/publish.js",
    "release": "node scripts/release.js"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "commander": "^9.3.0",
    "commitizen": "^4.2.5",
    "conventional-changelog-cli": "^2.2.2",
    "download-git-repo": "^3.0.2",
    "execa": "^5",
    "fs-extra": "^10.1.0",
    "inquirer": "^8.2.4",
    "leven": "^3",
    "node-emoji": "^1.11.0",
    "ora": "^5",
    "readline": "^1.3.0",
    "semver": "^7.3.7"
  }
}
